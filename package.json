{
  "name": "@sushiswap/subgraphs",
  "private": true,
  "workspaces": [
    "packages/*",
    "subgraphs/*"
  ],
  "scripts": {
    "build": "yarn workspaces run build",
    "codegen": "yarn workspaces run codegen",
    "prepare:rubyNewSchain": "yarn workspaces run prepare:rubyNewSchain",
    "run-graph-node": "cd graph-node && docker-compose up",
    "remove-graph-node": "cd graph-node && docker-compose down",
    "clean-graph-node": "rm -rf graph-node/data/"
  },
  "devDependencies": {
    "@graphprotocol/graph-cli": "^0.21.1",
    "@graphprotocol/graph-ts": "^0.20.0",
    "@sushiswap/core": "^1.4.2",
    "@sushiswap/prettier-config": "^0.1.0",
    "@typescript-eslint/eslint-plugin": "^4.0.0",
    "@typescript-eslint/parser": "^4.0.0",
    "ejs": "^3.1.6",
    "eslint": "^7.0.0",
    "eslint-config-prettier": "^6.0.0",
    "handlebars": "^4.7.7",
    "mustache": "^4.2.0",
    "prettier": "^2.0.0",
    "typescript": "^4.0.0"
  }
}
